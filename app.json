{
  "expo": {
    "name": "Project: MORPH!",
    "slug": "nature-lab",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./images/spotted.png",
    "splash": {
      "image": "./images/DSC_0079.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "edu.umich.mobile.morph", 
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "edu.umich.mobile.morph",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "assetExts": [
        "ttf",
        "mp4",
        "otf",
        "xml",
        "obj",
        "mtl"
      ]
    }
  }
}
